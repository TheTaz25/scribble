{
  "name": "scribble",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-scribble-lib.umd.cjs",
  "module": "./dist/vue-scribble-lib.js",
  "exports": {
    ".": {
      "import": "./dist/vue-scribble-lib.js",
      "require": "./dist/vue-scribble-lib.umd.cjs",
      "types": "./dist/src/index.d.ts"
    },
    "./style.css": "./dist/vue-scribble-lib.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build2": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@rushstack/eslint-patch": "^1.13.0",
    "@tsconfig/node20": "^20.1.6",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-tsc": "^3.1.1"
  }
}
